{
	"name": "run-in-roblox-studio",
	"globIgnorePaths": ["**/package.json", "**/tsconfig.json"],
	"emitLegacyScripts": true,
	"tree": {
		"$path": "out",
		"include": {
			"$path": "include"
		},
		"node_modules": {
			"$className": "Folder",
			"@rbxts": {
				"$path": "node_modules/@rbxts"
			}
		}
	}
}
