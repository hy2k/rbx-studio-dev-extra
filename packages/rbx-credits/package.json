{
	"name": "rbx-credits",
	"version": "0.0.1",
	"type": "module",
	"bin": {
		"rbx-credits": "bin/rbx-credits.mjs"
	},
	"files": [
		"bin",
		"dist",
		"!**/*.test.*",
		"plugin/rbx-credits.rbxm"
	],
	"scripts": {
		"start": "cross-env NODE_ENV=development node bin/rbx-credits.mjs --place rbx-credits.rbxl",
		"build": "concurrently -c auto \"npm:build:*\"",
		"build:ts": "tsc",
		"build:plugin": "npm run -prefix plugin build",
		"watch": "concurrently -c auto \"npm:watch:*\"",
		"watch:ts": "tsc -w --preserveWatchOutput",
		"watch:plugin": "npm run -prefix plugin watch",
		"watch:place": "rojo build -o rbx-credits.rbxl --watch",
		"serve": "rojo serve",
		"clean": "rimraf dist",
		"test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
		"test:watch": "npm run test -- --watch",
		"lint": "eslint src",
		"prepack": "npm run clean && tsc",
		"prepublishOnly": "npm run test && npm run lint",
		"postinstall": "npm i -prefix plugin"
	},
	"license": "MIT",
	"dependencies": {
		"cleye": "^1.3.2",
		"fastify": "^4.25.0",
		"ky": "^1.1.3",
		"open-rbxl": "^0.0.4",
		"pino": "^8.17.1",
		"roblox-studio-pathutil": "^0.0.3",
		"zod": "^3.22.4"
	}
}
