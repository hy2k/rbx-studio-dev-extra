{
	"name": "open-rbxl",
	"description": "Open rbxl/rbxlx place file in Roblox Studio",
	"version": "0.0.2",
	"type": "module",
	"main": "dist/index.js",
	"bin": {
		"open-rbxl": "dist/cli.js"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsc",
		"watch": "tsc -w --preserveWatchOutput",
		"clean": "rimraf dist",
		"test": "node --test",
		"test:watch": "node --test --watch",
		"prepack": "npm run clean && tsc -p tsconfig.build.json",
		"prepublishOnly": "npm run test"
	},
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^20.10.3",
		"rimraf": "^5.0.5",
		"typescript": "^5.3.2"
	},
	"dependencies": {
		"cleye": "^1.3.2"
	}
}
