{
	"name": "open-rbxl",
	"description": "Open rbxl/rbxlx place file in Roblox Studio",
	"version": "0.0.4",
	"type": "module",
	"main": "dist/index.js",
	"bin": {
		"open-rbxl": "bin/open-rbxl.mjs"
	},
	"files": [
		"bin",
		"dist",
		"!**/*.test.*"
	],
	"scripts": {
		"build": "tsc",
		"watch": "tsc -w --preserveWatchOutput",
		"clean": "rimraf dist",
		"test": "node --test",
		"test:watch": "node --test --watch",
		"lint": "eslint src",
		"prepack": "npm run clean && tsc",
		"prepublishOnly": "npm run test && npm run lint"
	},
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^20.10.3",
		"rimraf": "^5.0.5",
		"typescript": "^5.3.2"
	},
	"dependencies": {
		"cleye": "^1.3.2",
		"roblox-studio-pathutil": "^0.0.3"
	}
}
