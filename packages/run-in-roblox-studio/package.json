{
	"name": "run-in-roblox-studio",
	"version": "0.0.1",
	"type": "module",
	"bin": {
		"run-in-roblox-studio": "bin/run-in-roblox-studio.mjs"
	},
	"files": [
		"bin",
		"dist",
		"!**/*.test.*"
	],
	"scripts": {
		"start": "cross-env NODE_ENV=development node ./bin/run-in-roblox-studio.mjs --place place.rbxl --script script.server.lua",
		"dev": "concurrently -c auto \"npm:dev:*\"",
		"build": "tsc",
		"dev:ts": "tsc -w --preserveWatchOutput",
		"dev:rojo": "rojo build -o ./place.rbxl",
		"clean": "rimraf dist",
		"test": "node --test",
		"test:watch": "node --test --watch",
		"prepack": "npm run clean && tsc",
		"prepublishOnly": "npm run test"
	},
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^20.10.3",
		"concurrently": "^8.2.2",
		"cross-env": "^7.0.3",
		"typescript": "^5.3.2"
	},
	"dependencies": {
		"cleye": "^1.3.2",
		"fastify": "^4.24.3",
		"open-rbxl": "^0.0.3"
	}
}
