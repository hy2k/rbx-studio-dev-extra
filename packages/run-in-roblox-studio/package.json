{
	"name": "run-in-roblox-studio",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"bin": "./bin/cli.mjs",
	"scripts": {
		"dev": "concurrently -c auto \"npm:dev:*\"",
		"build": "tsc",
		"dev:ts": "tsc -w --preserveWatchOutput",
		"dev:rojo": "rojo build -o ./place.rbxl",
		"start": "node ./bin/cli.mjs --place place.rbxl --script script.server.lua"
	},
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^20.10.3",
		"concurrently": "^8.2.2",
		"typescript": "^5.3.2"
	},
	"dependencies": {
		"cleye": "^1.3.2",
		"fastify": "^4.24.3",
		"open-rbxl": "^0.0.3"
	}
}
